CREATE TABLE tb_careplans (
Id VARCHAR (256) DEFAULT NULL,
START VARCHAR (256) DEFAULT NULL,
STOP DATE (10) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) default NULL,
CODE VARCHAR (256) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL,
REASONCODE VARCHAR (256) DEFAULT NULL,
REASONDESCRIPTION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_conditions (
START DATE (10) DEFAULT NULL,
STOP DATE (10) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) DEFAULT NULL,
CODE INT (9) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_devices (
START TIMESTAMP (20) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) DEFAULT NULL,
CODE INT (9) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL,
UDI VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_disease (
START	VARCHAR (256) DEFAULT NULL,
STOP	VARCHAR (256) DEFAULT NULL,
PATIENT	VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER	VARCHAR (256) DEFAULT NULL,
CODE	VARCHAR (256) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL
);
