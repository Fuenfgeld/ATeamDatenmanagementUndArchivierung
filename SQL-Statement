CREATE TABLE tb_careplans (
Id VARCHAR (256) DEFAULT NULL,
START VARCHAR (256) DEFAULT NULL,
STOP DATE (10) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) default NULL,
CODE VARCHAR (256) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL,
REASONCODE VARCHAR (256) DEFAULT NULL,
REASONDESCRIPTION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_conditions (
START DATE (10) DEFAULT NULL,
STOP DATE (10) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) DEFAULT NULL,
CODE INT (9) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_devices (
START TIMESTAMP (20) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) DEFAULT NULL,
CODE INT (9) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL,
UDI VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_disease (
START	VARCHAR (256) DEFAULT NULL,
STOP	VARCHAR (256) DEFAULT NULL,
PATIENT	VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER	VARCHAR (256) DEFAULT NULL,
CODE	VARCHAR (256) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_medications (
START	TIMESTAMP (20) DEFAULT NULL,
STOP	TIMESTAMP (20) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
PAYER	VARCHAR (256) DEFAULT NULL,
ENCOUNTER	VARCHAR (256) DEFAULT NULL,
CODE	INT (7) DEFAULT NULL,
DESCRIPTION	VARCHAR (256) DEFAULT NULL,
BASE_COST VARCHAR (256) DEFAULT NULL,
PAYER_COVERAGE	VARCHAR (256) DEFAULT NULL,
DISPENSES	INT (3) DEFAULT NULL,
TOTALCOST	VARCHAR (256) DEFAULT NULL,
REASONCODE VARCHAR (256) DEFAULT NULL,
REASONDESCRIPTION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_observations (
DATE TIMESTAMP (20) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) DEFAULT NULL,
CODE BOOLEAN (4) DEFAULT NULL,
DESCRIPTION VARCHAR (4) DEFAULT NULL,
VALUE VARCHAR (256) DEFAULT NULL,
UNITS BOOLEAN (6) DEFAULT NULL,
TYPE BOOLEAN (7) DEFAULT NULL
);

CREATE TABLE tb_organizations (
Id VARCHAR (256) PRIMARY KEY NOT NULL,
NAME VARCHAR (256) DEFAULT NULL,
ADDRESS VARCHAR (256) DEFAULT NULL,
CITY VARCHAR (256) DEFAULT NULL,
STATE VARCHAR (256) DEFAULT NULL,
ZIP VARCHAR (256) DEFAULT NULL,
LAT VARCHAR (256) DEFAULT NULL,
LON VARCHAR (256) DEFAULT NULL,
PHONE VARCHAR (256) DEFAULT NULL,
REVENUE VARCHAR (256) DEFAULT NULL,
UTILIZATION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_patients (
Id VARCHAR (256) PRIMARY KEY NOT NULL,
BIRTHDATE DATE (10) DEFAULT NULL,
DEATHDATE DATE (10) DEFAULT NULL,
SSN VARCHAR (256) DEFAULT NULL,
DRIVERS VARCHAR (256) DEFAULT NULL,
PASSPORT VARCHAR (10) DEFAULT NULL,
PREFIX VARCHAR (4) DEFAULT NULL,
FIRST VARCHAR (256) DEFAULT NULL,
LAST VARCHAR (256) DEFAULT NULL,
SUFFIX VARCHAR (10) DEFAULT NULL,
MAIDEN VARCHAR (256) DEFAULT NULL,
MARITAL VARCHAR (1) DEFAULT NULL,
RACE VARCHAR (256) DEFAULT NULL,
ETHNICITY VARCHAR (256) DEFAULT NULL,
GENDER BOOLEAN (1) DEFAULT NULL,
BIRTHPLACE VARCHAR (256) DEFAULT NULL,
ADDRESS VARCHAR (256) DEFAULT NULL,
CITY VARCHAR (256) DEFAULT NULL,
STATE BOOLEAN (13) DEFAULT NULL,
COUNTY VARCHAR (256) DEFAULT NULL,
ZIP CHAR (4) DEFAULT NULL,
LAT VARCHAR (256) DEFAULT NULL,
LON VARCHAR (256) DEFAULT NULL,
HEALTHCARE_EXPENSES VARCHAR (256) DEFAULT NULL,
HEALTHCARE_COVERAGE VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_payer_transitions (
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
START_YEAR INT (4) DEFAULT NULL,
END_YEAR INT (4) DEFAULT NULL,
PAYER VARCHAR (256) DEFAULT NULL,
OWNERSHIP BOOLEAN (8) DEFAULT NULL
);

CREATE TABLE tb_payers (
Id VARCHAR (256) PRIMARY KEY NOT NULL,
NAME VARCHAR (256) DEFAULT NULL,
ADDRESS VARCHAR (256) DEFAULT NULL,
CITY VARCHAR (256) DEFAULT NULL,
STATE_HEADQUARTERED VARCHAR (2) DEFAULT NULL,
ZIP INT (5) DEFAULT NULL,
PHONE VARCHAR (256) DEFAULT NULL,
AMOUNT_COVERED VARCHAR (256) DEFAULT NULL,
AMOUNT_UNCOVERED VARCHAR (256) DEFAULT NULL,
REVENUE INT (9) DEFAULT NULL,
COVERED_ENCOUNTERS INT (4) DEFAULT NULL,
UNCOVERED_ENCOUNTERS VARCHAR (256) DEFAULT NULL,
COVERED_MEDICATIONS VARCHAR (2) DEFAULT NULL,
UNCOVERED_MEDICATIONS VARCHAR (2) DEFAULT NULL,
COVERED_PROCEDURES VARCHAR (3) DEFAULT NULL,
UNCOVERED_PROCEDURES VARCHAR (2) DEFAULT NULL,
COVERED_IMMUNIZATIONS VARCHAR (5) DEFAULT NULL,
UNCOVERED_IMMUNIZATIONS VARCHAR (256) DEFAULT NULL,
UNIQUE_CUSTOMERS VARCHAR (3) DEFAULT NULL,
QOLS_AVG VARCHAR (256) DEFAULT NULL,
MEMBER_MONTHS INT (5) DEFAULT NULL
);

CREATE TABLE tb_procedures (
DATE TIMESTAMP (20) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) DEFAULT NULL,
CODE INT (9) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL,
BASE_COST VARCHAR (256) DEFAULT NULL,
REASONCODE INT (9) DEFAULT NULL,
REASONDESCRIPTION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_providers (
Id VARCHAR (256) PRIMARY KEY NOT NULL,
ORGANIZATION VARCHAR (256) DEFAULT NULL,
NAME VARCHAR (256) DEFAULT NULL,
GENDER BOOLEAN (1) DEFAULT NULL,
SPECIALITY VARCHAR (256) DEFAULT NULL,
ADDRESS VARCHAR (256) DEFAULT NULL,
CITY VARCHAR (256) DEFAULT NULL,
STATE VARCHAR (2) DEFAULT NULL,
ZIP VARCHAR (256) DEFAULT NULL,
LAT VARCHAR (256) DEFAULT NULL,
LON VARCHAR (256) DEFAULT NULL,
UTILIZATION VARCHAR (256) DEFAULT NULL
);

CREATE TABLE tb_supplies (
DATE DATE (10) DEFAULT NULL,
PATIENT VARCHAR (256) PRIMARY KEY NOT NULL,
ENCOUNTER VARCHAR (256) DEFAULT NULL,
CODE INT (9) DEFAULT NULL,
DESCRIPTION VARCHAR (256) DEFAULT NULL,
QUANTITY INT (2) DEFAULT NULL
);
